{
  "extends": [
    "eslint:recommended", 
    "plugin:prettier/recommended", 
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "no-undef": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 13,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
